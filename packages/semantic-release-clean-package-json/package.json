{
    "name": "@anolilab/semantic-release-clean-package-json",
    "version": "4.2.3-alpha.1",
    "description": "Clean package.json before publish by removing unnecessary properties.",
    "keywords": [
        "anolilab",
        "semantic-release-clean-package-json",
        "semantic-release",
        "semantic-release-plugin",
        "clean",
        "package.json",
        "publish",
        "prepare",
        "postpublish",
        "cleanup",
        "optimize",
        "minify",
        "strip",
        "remove",
        "sanitize",
        "normalize",
        "package-cleanup",
        "publish-cleanup",
        "npm-publish",
        "registry"
    ],
    "homepage": "https://github.com/anolilab/semantic-release/tree/main/packages/semantic-release-clean-package-json",
    "bugs": {
        "url": "https://github.com/anolilab/semantic-release/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anolilab/semantic-release.git",
        "directory": "packages/semantic-release-clean-package-json"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "CHANGELOG.md",
        "README.md",
        "dist"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint .",
        "lint:eslint:fix": "eslint . --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@semantic-release/error": "catalog:cli",
        "@visulima/fs": "catalog:prod",
        "@visulima/path": "catalog:node",
        "type-fest": "catalog:dev"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "catalog:lint",
        "@anolilab/prettier-config": "catalog:lint",
        "@anolilab/semantic-release-pnpm": "4.0.0-alpha.1",
        "@secretlint/secretlint-rule-preset-recommend": "catalog:lint",
        "@semantic-release/changelog": "catalog:cli",
        "@semantic-release/commit-analyzer": "catalog:cli",
        "@semantic-release/exec": "catalog:cli",
        "@semantic-release/git": "catalog:cli",
        "@semantic-release/github": "catalog:cli",
        "@semantic-release/release-notes-generator": "catalog:cli",
        "@types/node": "catalog:types",
        "@types/semantic-release__error": "catalog:types",
        "@visulima/packem": "catalog:dev",
        "@vitest/coverage-v8": "catalog:test",
        "@vitest/ui": "catalog:test",
        "esbuild": "catalog:build",
        "eslint": "catalog:lint",
        "prettier": "catalog:lint",
        "rimraf": "catalog:node",
        "secretlint": "catalog:lint",
        "semantic-release": "catalog:cli",
        "tempy": "catalog:dev",
        "typescript": "catalog:tsc",
        "vitest": "catalog:test"
    },
    "engines": {
        "node": "^22.14.0 || >=24.10.0"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
