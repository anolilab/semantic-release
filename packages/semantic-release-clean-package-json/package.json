{
    "name": "@anolilab/semantic-release-clean-package-json",
    "version": "3.0.3",
    "description": "Clean package.json before publish by removing unnecessary properties",
    "keywords": [
        "anolilab",
        "semantic-release-clean-package-json",
        "semantic-release",
        "clean",
        "package.json",
        "publish"
    ],
    "homepage": "https://github.com/anolilab/semantic-release/tree/main/packages/semantic-release-clean-package-json",
    "bugs": {
        "url": "https://github.com/anolilab/semantic-release/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anolilab/semantic-release.git",
        "directory": "packages/semantic-release-clean-package-json"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "files": [
        "CHANGELOG.md",
        "README.md",
        "dist"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint .",
        "lint:eslint:fix": "eslint . --fix",
        "lint:package-json": "publint --strict",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@semantic-release/error": "^4.0.0",
        "@visulima/fs": "^3.1.5",
        "@visulima/path": "^1.4.0"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.2.25",
        "@anolilab/semantic-release-pnpm": "2.0.3",
        "@secretlint/secretlint-rule-preset-recommend": "^11.2.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "13.0.1",
        "@semantic-release/exec": "^7.1.0",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.5",
        "@semantic-release/release-notes-generator": "14.1.0",
        "@types/node": "24.3.1",
        "@types/semantic-release": "^21.1.0",
        "@types/semantic-release__error": "^3.0.3",
        "@visulima/packem": "2.0.0-alpha.19",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "esbuild": "0.25.9",
        "eslint": "9.35.0",
        "rimraf": "^6.0.1",
        "secretlint": "11.2.0",
        "semantic-release": "^24.2.7",
        "tempy": "^3.1.0",
        "type-fest": "^4.41.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=20.8.1"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
